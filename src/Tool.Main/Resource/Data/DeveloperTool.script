	,MenuName TEXT(100)
	,ParentCode TEXT(100)
	,Assembly TEXT(100)
	,NameSpace TEXT(100)
	,EntityName TEXT(200)
	,LEVEL INTEGER
	,IfEnd INTEGER
	);

